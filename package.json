{
  "name": "translator-sdk",
  "engines": {
    "node": ">10.0.0 <13.0.0"
  },
  "version": "1.0.0",
  "description": "NodeJs Translator SDK for Darwin platform",
  "main": "index.js",
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "dependencies": {
    "@delos-tech/darwin-logger": "^1.0.1",
    "async-mqtt": "^2.6.1",
    "eslint-plugin-security": "^1.4.0",
    "livr": "^2.2.2",
    "mole-rpc": "0.3.2",
    "mole-rpc-transport-mqtt": "0.3.0",
    "save-dev": "0.0.1-security"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.4.0",
    "eslint-config-webbylab": "^5.4.1",
    "eslint-plugin-fetch": "0.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-more": "^1.0.0",
    "eslint-plugin-no-require-lodash": "^1.1.0",
    "eslint-plugin-prefer-spread": "^1.0.3",
    "eslint-plugin-react": "^7.20.3",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0"
  },
  "scripts": {
    "test": "MODE=test mocha --ui qunit tests/*",
